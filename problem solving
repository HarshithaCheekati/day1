        //Write a program to check wheather the number is divisible by 2 and 3 and both (take prompt)
        let a=parseInt(prompt("Enter the value" ))
         if(a % 2 === 0 && a % 3 === 0){
             document.write("It is divisible by 2 and 3 both")
         }else{
             document.write("It is not divisible by 2 and 3 both")
         }

        //write a program to find the largest number out of three numbers expected from the user
